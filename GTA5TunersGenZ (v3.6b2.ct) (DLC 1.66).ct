       if tbl_PlayerCount &lt; 1 or is_TableEmpty(tbl_Players) == true then
         playerList.Items.clear()
	     do_PlayerReset()
      else
        if playerList.Items.Count == 0 then
           playerList.Items.clear()
           for k, v in pairs(tbl_Players) do
             local usename = check_Player(k, v[2], v[3], v[4], v[5], v[6])
             playerList.Items.add(usename)
           end
         else
          local useIndex = 0

          for k, v in pairs(tbl_Players) do
            useIndex = k - 1
            if not v[6] or v[6] ~= 2 then v[6] = -1 end

            local usename = check_Player(k, v[2], v[3], v[4], v[5], v[6])

            if k &gt; playerList.Items.Count then
              playerList.Items.add(usename)
            elseif k &lt;= playerList.Items.Count and playerList.Items[useIndex] ~= usename then
              playerList.Items[useIndex] = usename
            end
          end

          if tbl_Players and playerList.Items.Count &gt; #tbl_Players then
            for i = playerList.Items.Count, #tbl_Players, -1 do
              playerList.Items.Delete(i)
            end
          end
        end
