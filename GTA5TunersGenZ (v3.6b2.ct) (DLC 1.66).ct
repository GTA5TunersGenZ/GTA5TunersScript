function setStat(stat,value)
  local char,hash
  if not stat:match("MP0") or not stat:match("MP1") then
    char = "MP"..GG('i',1574918).."_"
    hash = joaat(char..stat)
  else hash = joaat(stat) end

  local oldhash = GG('i',1665454+4)
  local oldvalue = GG('i',1010831+5525)
