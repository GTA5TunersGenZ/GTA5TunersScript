  end
end

function Outfits_FileHandler(ToDo,SlotID,Name)
  local FilePath_Name = TrainerOrigin..'GTA_VenomKY_Outfits.ini'

  local memBuffer = {}
  local temp = ""

  if ToDo == 'write' then
    local file = io.open(FilePath_Name, "a")
    if not file then print("Failed To Create Or Open The File --&gt; "..FilePath_Name) return false end

	local LocalName = Name

	if not Name or Name == "" then
	  local RandNum = math.random(1,999)
	  LocalName = "SavedOutfit_"..RandNum
    elseif #Name &gt; 19 then LocalName = string.sub(Name,1,19)
	end

	for i = 3,14 do
	  if i &gt; 3 then
	    memBuffer = getSlotInfo(1,SlotID,i)
		temp = string.format("%s;{%s,%s}",temp,memBuffer[1],memBuffer[2])
	  end

	  if i &lt; 6 then
	    memBuffer= getSlotInfo(2,SlotID,i)

        if i==3 then temp = string.format("{%s,%s}",memBuffer[1],memBuffer[2])
		else temp = string.format("%s;{%s,%s}",temp,memBuffer[1],memBuffer[2])
	    end
	  elseif i == 9 or i == 10 then
	    memBuffer = getSlotInfo(2,SlotID,i)
		temp = string.format("%s;{%s,%s}",temp,memBuffer[1],memBuffer[2])
	  end
	end

    file:write(string.format("%s --&gt; %s\n",LocalName,temp))
    io.close(file)
	return true
  elseif ToDo == 'read' then
    local Start,End
	local Count = 0
	local _Name = nil

    for line in io.lines(FilePath_Name) do
	  Start,End = string.find(line,"--&gt;")
	  if not Start or not End then return false,"Error Can't Find SavedOutfit Name !!" end

	  _Name = string.sub(line,1,Start-2)
	  if not _Name then return false,"Error Can't Find SavedOutfit Name !!" end
	  memBuffer[_Name]= {}
	  line = string.sub(line,End+2,-1)
	  Count = 0

      while true do
	    Start,End = string.find(line,"%b{}")
		if not Start or not End then break end

        temp = string.sub(line,Start+1,End-1)
		Start = string.find(line,",")
		Count = Count +1
		memBuffer[_Name][Count] = {tonumber(string.sub(temp,1,Start-2)),tonumber(string.sub(temp,Start,-1))}
		line = string.sub(line,End+2,-1)
	  end


      if Count ~= 16 then
	    print("Warning: ".._Name.."Data Is Corrupted !!")
		memBuffer[_Name] = nil
	  end
	end

    if not _Name then return false,"Empty File !!" end
	return true,memBuffer
  end
end

function SetOutfitValues(SlotID,SlotOffset)
  local isSuccessful,Output = Outfits_FileHandler('read')
  --if not isSuccessful then print(Output) return end

  local OutfitsDropDownList = createStringlist()
  local OutfitName
  for OutfitName, Values in pairs(Output) do
    if #OutfitName &gt; 19 then OutfitName = string.sub(OutfitName,1,19) end
	OutfitsDropDownList.add(OutfitName)
  end

  local SelectedIDX,SelectedOutfit = showSelectionList('## Wardrobe ##','Choose Outfit',OutfitsDropDownList,false)

  if  SelectedIDX == -1 then --[[print("Error: No Selection")--]] return end

  _writeString(GA(oWardrobeG+(0*oWPointA)+oWPointB+oWComponent+(SlotID*13)+SlotOffset),SelectedOutfit,20)

  local Count = 0
  for i = 3,14 do
    if i &gt; 3 then
	  Count = Count + 1
	  setSlotInfo(1,SlotID,i,Output[SelectedOutfit][Count])
	end

	if i &lt; 6 then
	  Count = Count + 1
	  setSlotInfo(2,SlotID,i,Output[SelectedOutfit][Count])
	elseif i == 9 or i == 10 then
	  Count = Count + 1
	  setSlotInfo(2,SlotID,i,Output[SelectedOutfit][Count])
	end
  end
end

