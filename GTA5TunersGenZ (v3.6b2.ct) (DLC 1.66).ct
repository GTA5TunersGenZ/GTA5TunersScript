    local function getWPBlip()
      local ptr = getAddressSafe("BlipPTR")

      for i = 2000, 1, -1
      do
        local blip = readPointer(ptr + (i * 8))

        if (blip &gt; 0) and (8 == readInteger(blip + 0x40)) and (84 == readInteger(blip + 0x48)) then return blip end
      end
    end

    local function setBlipOnPlayer(CPed)
      local WPblip = getWPBlip()

      if not WPblip then return false end

      local x,y,z = getPlayerCoords(CPed)

      writeFloat(WPblip + 0x10,x)
      writeFloat(WPblip + 0x14,y)

      return true
    end

