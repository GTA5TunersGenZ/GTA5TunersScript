   SG('i',oVMCreate+27+69, mod[14][1]) -- Wheel type
   SG('i',oVMCreate+27+74, mod[18][1]) -- Neon R 0-255
   SG('i',oVMCreate+27+75, mod[18][2]) -- G
   SG('i',oVMCreate+27+76, mod[18][3]) -- B
