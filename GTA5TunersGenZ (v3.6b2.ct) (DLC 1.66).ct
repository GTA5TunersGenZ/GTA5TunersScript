function check_if_me(CPed)
  local CPlayerInfo = isAddressSafe(CPed + pCPlayerInfo, 3)
  if not CPlayerInfo then return -1 end

  local CPed = isAddressSafe(CPlayerInfo + pCNetPed, 3)
  if not CPed or CPed == 0 then return -1 end

  local RetValue = false
  local swimBackup = readFloat(CPlayerInfo + oSwimSpeed)
  local runspeedBackup = readFloat(CPlayerInfo + oRunSpeed)
  local swimRand = math.random(220,362) + .5
  local runSpeedRand = math.random(1,3) + .7

  writeFloat("[[[WorldPTR]+pCPed]+pCPlayerInfo]+oSwimSpeed", swimRand)
  writeFloat("[[[WorldPTR]+pCPed]+pCPlayerInfo]+oRunSpeed", runSpeedRand)

  local swimTest = round(readFloat(CPlayerInfo + oSwimSpeed), 1)
  local runTest = round(readFloat(CPlayerInfo + oRunSpeed), 1)

  if tonumber(swimTest) == tonumber(swimRand) and tonumber(runTest) == tonumber(runSpeedRand) then RetValue = true end

  writeFloat("[[[WorldPTR]+pCPed]+pCPlayerInfo]+oSwimSpeed", swimBackup)
  writeFloat("[[[WorldPTR]+pCPed]+pCPlayerInfo]+oRunSpeed", runspeedBackup)

  return RetValue
end

function check_PlayerTable(CPed, n_name, r_rid)
  local timestamp = tonumber(os.time(os.date("!*t")))
  local datestamp = os.date("%I:%M %p %x")
  local x,y,z = getPlayerCoords(CPed)
  local is_modder = 0
  local gotmatch = 0
  local countt  = 0
  local checkIfGod = 0
  local skipChecks = 0

  for k, v in pairs(tbl_origPlayersList) do
    if tonumber(v[4]) == 2 then goto topbottom end

    if tonumber(markMyRid) == tonumber(v[3]) and tonumber(r_rid) == tonumber(v[3]) and tonumber(myCPed) ~= tonumber(CPed) then
      local checkIFme = check_if_me(CPed)

      if checkIFme == -1 then
        skipChecks = 1
      elseif checkIFme == 1 then
        myCPed = CPed
      end
    end

    if tonumber(v[1]) == CPed then
      if skipChecks  == 1 or myCPed == CPed then
        if tonumber(v[3]) ~= tonumber(r_rid) and tonumber(markMyRid) == tonumber(v[3]) then
          markMyRid = r_rid
          tbl_origPlayersList[k][3] = r_rid
        end

        goto SkipChecks
      end

      if tonumber(v[5]) == 2 then
        is_modder = 2
        tbl_origPlayersList[k][19] = 0
      elseif tonumber(v[4]) == 1 then
        if v[2] ~= n_name or tonumber(v[3]) ~= r_rid then
         tbl_origPlayersList[k][14] = 0

         if aLogPlayers == 1 then
           local logBuffer = '['..v[3]..'] '..v[2]

           if v[2] ~= n_name then
             logBuffer = logBuffer..' [Modder Detected, Spoofing Name: '..n_name..']'

             log_append(logBuffer)
            end

            if tonumber(v[3]) ~= r_rid then
              logBuffer = logBuffer..' [Modder Detected, Spoofing Rid: '..r_rid..']'

              log_append(logBuffer)
            end
          end
        end

        local stopG = 0
        checkIfGod = readBytes(CPed + oGod)

        if tonumber(v[5]) == 1 then
          is_modder = 1
          tbl_origPlayersList[k][19] = 0
        end

        if tonumber(v[1]) and godModeCheck == 0 and checkIfGod == 1 and is_modder ~= 1 and is_modder ~= 2 then
          if tonumber(v[19]) == 0 then
            tbl_origPlayersList[k][20] = tonumber(timestamp) + isg_timeout
            tbl_origPlayersList[k][13] = 0
            tbl_origPlayersList[k][19] = 1
            tbl_origPlayersList[k][11] = x
            tbl_origPlayersList[k][12] = y
          else
            if tonumber(x) == tonumber(v[11]) and tonumber(y) == tonumber(v[12]) then
              tbl_origPlayersList[k][20] = tonumber(timestamp) + isg_timeout
              tbl_origPlayersList[k][13] = 0
              tbl_origPlayersList[k][19] = 1
              tbl_origPlayersList[k][11] = x
              tbl_origPlayersList[k][12] = y
            elseif tonumber(v[19]) &gt;= 3 then
              stopG = 1
              tbl_origPlayersList[k][13] = 1
            else
              tbl_origPlayersList[k][19] = tbl_origPlayersList[k][19] + 1
              tbl_origPlayersList[k][11] = x
              tbl_origPlayersList[k][12] = y
            end
          end
        end

        if tonumber(v[5]) == 1 then
          is_modder = 1
          tbl_origPlayersList[k][13] = 0
          tbl_origPlayersList[k][19] = 0
        elseif is_modder ~= 2 and stopG == 1 then
          if tonumber(v[13]) == 1 and tonumber(timestamp) &gt;= tonumber(v[20]) then
            if tonumber(x) ~= tonumber(v[11]) or tonumber(y) ~= tonumber(v[12]) then
              is_modder = 1
              tbl_origPlayersList[k][19] = 0

              if aLogPlayers == 1 then
                local logBuffer = '['..v[3]..'] '..v[2]..' [Modder Detected: (Godmode &amp; moving cooridinates), Current Name/Rid if changed: '..n_name..' '..r_rid..']'
                log_append(logBuffer)
              end
            end
          elseif tonumber(v[13]) == 1 and tonumber(timestamp) &lt; tonumber(v[20]) then
            tbl_origPlayersList[k][11] = x
            tbl_origPlayersList[k][12] = y
          end
        end
      end

      ::SkipChecks::

      local new_join = v[4]

      if tonumber(v[6]) &gt; timestamp then
        timestamp = v[6]
      elseif tonumber(v[4]) ~= 1 then
        new_join = 1
        tbl_origPlayersList[k][13] = 0
        tbl_origPlayersList[k][19] = 0
      end

      if checkIfGod ~= 1 then
        tbl_origPlayersList[k][13] = 0
        tbl_origPlayersList[k][19] = 0
      end

      tbl_origPlayersList[k][4] = new_join
      tbl_origPlayersList[k][5] = is_modder
      tbl_origPlayersList[k][6] = timestamp
      tbl_origPlayersList[k][7] = x
      tbl_origPlayersList[k][8] = y
      tbl_origPlayersList[k][9] = z

      return k
    end

    countt = countt + 1

    ::topbottom::
  end

  if tonumber(r_rid) == tonumber(markMyRid) and check_if_me(CPed) == true then myCPed = CPed end

  if countt == 0 then countt = 1 end

  timestamp = timestamp + jp_timeout

  table.insert(tbl_origPlayersList, countt, {CPed, n_name, r_rid, 0, 0, timestamp, x, y, z, datestamp, x, y, 0, 0, 0, 0, 0, 0, 0, 0})

  return countt
end

function getPlayerList()
  getPlayerCalled = true

  local CNetworkPlayerMgr=readPointer("PlayerCountPTR")
  if not CNetworkPlayerMgr then
    getPlayerCalled = false
    return
  end

  local hostPointer = readPointer("NetworkInfoPTR")
  local whoIsHost = 0
  local tbl_NoRagDoll = {}
  local tbl_Modder = {}
  local tbl_GoodBoys = {}
  local tbl_GodMode = {}
  local tbl_IsHost = {}
  local tbl_IsME = {}
  local IsMe = nil
  local icount = 1

  tbl_Players = {}
  tbl_PlayersAll = {}
  tbl_PlayerCount = 0

  mark_MYRid()

  for i=0,32,1 do
    local CNetGamePlayer = readPointer(CNetworkPlayerMgr + oNumPlayers + (i*8))
    if not CNetGamePlayer then goto continue end
    local CPlayerInfo = readPointer(CNetGamePlayer + pCNetPlayerInfo)
    if not CPlayerInfo then goto continue end
    local CPed = readPointer(CPlayerInfo + pCNetPed)
    if not CPed or CPed == 0 then goto continue end

    -- Get Rid and Name

    local ORGRid = readPointer(CPlayerInfo + oRid)
    local ORGName = readString(CPlayerInfo + oName)
    local Name = '['..ORGRid..'] '..ORGName

    -- Get Host Information.

    local isHost = -1

    if hostPointer then
      local isHostTok1 = readPointer(hostPointer + oHostNetToken)
      local isHostTok2 = readPointer(CPlayerInfo + oHostPlayerToken)
      if isHostTok1 == isHostTok2 then isHost = 1 end
    end

    if isHost ~= -1 then
      Name = Name..' (Host)'
      whoIsHost = CPed
    end

    local cd = check_PlayerTable(CPed, ORGName, ORGRid)

    if is_TableEmpty(tbl_origPlayersList) == true then
      getPlayerCalled = false
      return
    end

    table.insert(tbl_PlayersAll,{i, Name,CPed,ORGName,ORGRid})
    tbl_PlayerCount = tbl_PlayerCount + 1

    if tbl_origPlayersList[cd][4] == 1 then
      if tbl_origPlayersList[cd][1] == myCPed then
        Name = Name..' (Me)'
        IsMe = myCPed
      end

      local tt = is_tracked(tbl_origPlayersList[cd][2], tbl_origPlayersList[cd][3])
      if tt then Name = Name..' (Tracking: '..tt..')' end

      local int_gotmod,int_gotgod,int_gotrag=0,0,0
      local modder_prefix = ""

      if tonumber(tbl_origPlayersList[cd][5]) == 1 then
        int_gotmod = 1
        modder_prefix = ' (Modder'
      elseif tonumber(tbl_origPlayersList[cd][5]) == 2 then
        int_gotmod = 1
        modder_prefix = ' (Modder/Spoofer'
      end

      if readBytes(CPed + oGod) == 1 then int_gotgod = 1 end
      if readBytes(CPed + oRagdoll) ~= 32 then int_gotrag = 1 end

      if int_gotmod == 1 and int_gotgod == 1 and int_gotrag == 1 then
        Name = Name..modder_prefix..'/GodMode/NoRagDoll)'
        table.insert(tbl_Modder,{i, Name,CPed,ORGName,ORGRid})
      elseif int_gotmod == 0 and int_gotgod == 1 and int_gotrag == 1 then
        Name = Name..' (GodMode/NoRagDoll)'
        table.insert(tbl_GodMode,{i, Name,CPed,ORGName,ORGRid})
      elseif int_gotmod == 0 and int_gotgod == 1 and int_gotrag == 0 then
        Name = Name..' (GodMode)'
        table.insert(tbl_GodMode,{i, Name,CPed,ORGName,ORGRid})
      elseif int_gotmod == 0 and int_gotgod == 0 and int_gotrag == 1 then
        Name = Name..' (NoRagDoll)'
        table.insert(tbl_NoRagDoll,{i, Name,CPed,ORGName,ORGRid})
      elseif int_gotmod == 1 and int_gotgod == 0 and int_gotrag == 0 then
        Name = Name..modder_prefix..')'
        table.insert(tbl_Modder,{i, Name,CPed,ORGName,ORGRid})
      elseif int_gotmod == 1 and int_gotgod == 0 and int_gotrag == 1 then
        Name = Name..modder_prefix..'/NoRagDoll)'
        table.insert(tbl_Modder,{i, Name,CPed,ORGName,ORGRid})
      elseif int_gotmod == 1 and int_gotgod == 1 and int_gotrag == 0 then
        Name = Name..modder_prefix..'/GodMode)'
        table.insert(tbl_Modder,{i, Name,CPed,ORGName,ORGRid})
      elseif int_gotmod ~= 1 and int_gotmod ~= 2 and int_gotgod ~= 1 and int_gotrag ~= 1 then
        table.insert(tbl_GoodBoys,{i, Name,CPed,ORGName,ORGRid})
      end

      if whoIsHost == CPed then table.insert(tbl_IsHost,{i, Name,CPed,ORGName,ORGRid}) end
      if IsMe == CPed then table.insert(tbl_IsME,{i, Name,CPed,ORGName,ORGRid}) end
    end

    ::continue::
  end

  local function is_SkipHost(compCPed)
    if whoIsHost == compCPed or IsMe == compCPed then return true end
    return false
  end

  if not is_TableEmpty(tbl_IsHost) then
    if is_TableEmpty(tbl_IsME) or not is_TableEmpty(tbl_IsME) and tbl_IsME[1][3] ~= tbl_IsHost[1][3] then
      table.insert(tbl_Players,{icount,tbl_IsHost[1][2],tbl_IsHost[1][3],tbl_IsHost[1][4],tbl_IsHost[1][5]})
      icount = icount + 1
    end
  end

  if not is_TableEmpty(tbl_IsME) then
    table.insert(tbl_Players,{icount,tbl_IsME[1][2],tbl_IsME[1][3],tbl_IsME[1][4],tbl_IsME[1][5]})
    icount = icount + 1
  end

  for i, p in pairs(tbl_Modder) do
    if is_SkipHost(p[3]) == false then
      table.insert(tbl_Players,{icount,p[2],p[3],p[4],p[5]})
      icount = icount + 1
    end
  end

  for i, p in pairs(tbl_GodMode) do
    if is_SkipHost(p[3]) == false then
      table.insert(tbl_Players,{icount,p[2],p[3],p[4],p[5]})
      icount = icount + 1
    end
  end

  for i, p in pairs(tbl_NoRagDoll) do
    if is_SkipHost(p[3]) == false then
      table.insert(tbl_Players,{icount,p[2],p[3],p[4],p[5]})
      icount = icount + 1
    end
  end

  for i, p in pairs(tbl_GoodBoys) do
    if is_SkipHost(p[3]) == false then
      table.insert(tbl_Players,{icount,p[2],p[3],p[4],p[5]})
      icount = icount + 1
    end
  end

  local tbl_Joined = { }
  local tbl_Parted = { }

  for i, p in pairs(tbl_origPlayersList) do
    if tonumber(p[4]) == 2 then goto bottomtop end

    local foundit = -1
    local stname

    if tonumber(p[1]) == whoIsHost then stname = '['..p[3]..'] '..p[2]..' (Host)'
    else stname = '['..p[3]..'] '..p[2] end

    local tt = is_tracked(p[2], p[3])
    if tt then stname = stname..' (Tracking: '..tt..')' end

    for k, v in pairs(tbl_PlayersAll) do
      if tonumber(v[3]) == tonumber(p[1]) then
        foundit = 1
        break
      end
    end

    if tonumber(p[1]) == tonumber(myCPed) then stname = stname..' (Me)' end

    if foundit == 1 and tonumber(p[4]) == 0 then
      stname = stname..' has joined. (Processing..)'

      if tbl_origPlayersList[i][15] ~= 1 and aLogPlayers == 1 then log_append(stname) end

      table.insert(tbl_Joined,{i,stname,p[1],p[2],p[3],1})

      tbl_origPlayersList[i][15] = 1
    elseif foundit == -1 and tonumber(p[4]) == 1 then
      if tonumber(myCPed) == tonumber(p[1]) then myCPed = -1 end

      local timestamp = tonumber(os.time(os.date("!*t")))
      local sub1 = timestamp - pj_timeout

      if tonumber(p[6]) &gt; sub1 then
        stname = stname..' has left this session.'

        if tbl_origPlayersList[i][15] ~= 2 and aLogPlayers == 1 then log_append(stname) end

        table.insert(tbl_Parted,{i,stname,p[1],p[2],p[3],2})

        tbl_origPlayersList[i][15] = 2
      elseif tonumber(p[6]) &lt;= sub1 then
        tbl_origPlayersList[i][4] = 2
      end
    end
    ::bottomtop::
   end

   for i, p in pairs(tbl_Joined) do
     table.insert(tbl_Players,{icount,p[2],p[3],p[4],p[5],p[6]})
     icount = icount + 1
   end

   for i, p in pairs(tbl_Parted) do
     table.insert(tbl_Players,{icount,p[2],p[3],p[4],p[5],p[6]})
     icount = icount + 1
   end

   getPlayerCalled = false
end

