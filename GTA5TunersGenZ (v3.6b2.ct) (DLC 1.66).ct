    local function checkDup(tpName, XCoord, YCoord, ZCoord)
      for k,v in pairs(tbl_MyLocations) do
        if v[1] == tpName and tonumber(v[2]) == tonumber(XCoord) and tonumber(v[3]) == tonumber(YCoord) and tonumber(v[4]) == tonumber(ZCoord) then return true end
