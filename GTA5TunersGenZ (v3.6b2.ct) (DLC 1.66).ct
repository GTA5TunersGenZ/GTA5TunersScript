    TCOMP(ACCombox1,{Left=10,Top=11,Width=280,Height=33,DropDownCount=20,Align='alNone',Enabled=true,Visible=true,Anchors='[akTop,akLeft]',BidiMode='bdLeftToRight',CharCase='ecNormal',Color=536870912,Style="csDropDownList"})
    TCOMP(ACCombox1,{Text=acprefix})
    local GCCombox1=createComboBox(AutoMenus)
    TCOMP(GCCombox1,{Left=10,Top=45,Width=280,Height=33,DropDownCount=20,Align='alNone',Enabled=true,Visible=true,Anchors='[akTop,akLeft]',BidiMode='bdLeftToRight',CharCase='ecNormal',Color=536870912,Style="csDropDownList"})
    TCOMP(GCCombox1,{Text=aKprefix})
    local GCCombox2=createComboBox(AutoMenus)
    TCOMP(GCCombox2,{Left=10,Top=79,Width=280,Height=33,DropDownCount=20,Align='alNone',Enabled=true,Visible=true,Anchors='[akTop,akLeft]',BidiMode='bdLeftToRight',CharCase='ecNormal',Color=536870912,Style="csDropDownList"})
    TCOMP(GCCombox2,{Text=aUprefix})

    local function set_acPrefix()
      if test == 0 then aConnect = 0
      elseif test == 1 or test == -1 then aConnect = 1
      elseif test == 2 then aConnect = 2
      elseif test == 3 then aConnect = 3
      elseif test == 4 then aConnect = 4
      end

      if test == 0 then acprefix = "AutoConnect (Currently: Turned Off)"
      elseif test == 1 or test == -1 then acprefix = "AutoConnect (Currently: Solo Mode)"
      elseif test == 2 then acprefix = "AutoConnect (Currently: Online Mode)"
      elseif test == 3 then acprefix = "AutoConnect (Currently: Landing Page)"
      elseif test == 4 then acprefix = "AutoConnect (Currently: New Public)"
      end

      ACCombox1.Items.clear()

      if test == 0 then ACCombox1.items.add(acprefix)
      else ACCombox1.items.add("Turn Off")
      end

      if test == 1 or test == -1 then
        ACCombox1.items.add(acprefix)
        test = 1
      else ACCombox1.items.add("Solo Mode")
      end

      if test == 2 then ACCombox1.items.add(acprefix)
      else ACCombox1.items.add("Online Mode")
      end

      if test == 3 then ACCombox1.items.add(acprefix)
      else ACCombox1.items.add("Landing Page")
      end

      if test == 4 then ACCombox1.items.add(acprefix)
      else ACCombox1.items.add("New Public")
      end

      ACCombox1.ItemIndex=test
    end

    set_acPrefix()

    ACCombox1.OnChange=function()
      local id = ACCombox1.ItemIndex

      if (id == 0) then
        test = 0
        set_acPrefix()
        do_ConfigSave(1, 0)
        if ISGTARunning() == true and getAddressSafe("SettingsPTRs+oStartupFlow") and readInteger("SettingsPTRs+oStartupFlow") == 0 then writeInteger("SettingsPTRs+oStartupFlow", 1) end
      elseif (id == 1) then
        test = 1
        set_acPrefix()
        do_ConfigSave(1, 1)
      elseif (id == 2) then
        test = 2
        set_acPrefix()
        do_ConfigSave(1, 2)
      elseif (id == 3) then
        test = 3
        set_acPrefix()
        do_ConfigSave(1, 3)
      elseif (id == 4) then
        test = 4
        set_acPrefix()
        do_ConfigSave(1, 4)
      end
    end

    local gtest = get_Config(2)

    local function setAFKStatus()
      if gtest ~= 0 and gtest ~= 1 or gtest == 0 then aKprefix = "Anti-AFK (Currently: Turned Off)"
      elseif gtest == 1 then aKprefix = "Anti-AFK (Currently: Turned On)" end

      GCCombox1.Items.clear()

      if gtest == 1 then
        GCCombox1.items.add(aKprefix)
        GCCombox1.items.add("Turn Off Anti-AFK")
      else
        GCCombox1.items.add("Turn On Anti-AFK")
        GCCombox1.items.add(aKprefix)
      end

      if gtest == 1 then gtest = 0
      else gtest = 1
      end

      GCCombox1.ItemIndex = gtest
    end

    setAFKStatus()

    GCCombox1.OnChange=function()
      local id = GCCombox1.ItemIndex

      if (id == 1) then
        do_ConfigSave(2, 0)
        run_antiIdle(0)
        aFKtoggle = 0
        gtest = 0
        setAFKStatus()
      elseif (id == 0) then
        do_ConfigSave(2, 1)
        run_antiIdle(1)
        aFKtoggle = 1
        gtest = 1
        setAFKStatus()
      end
    end

    local Vtest = get_Config(5)

    local function set_UnlimWPPrefix()
      if Vtest == 0 or Vtest == -1 then aAmmoMode = 0
      elseif Vtest == 1 then aAmmoMode = 1
      elseif Vtest == 2 then aAmmoMode = 2
      elseif Vtest == 3 then aAmmoMode = 3
      end

      if Vtest == 0 or Vtest == -1 then aUprefix = "Ammo Mode (Currently: Turned Off)"
      elseif Vtest == 1 then aUprefix = "Ammo Mode (Currently: Unlimited Ammo)"
      elseif Vtest == 2 then aUprefix = "Ammo Mode (Currently: Unlimited Clip)"
      elseif Vtest == 3 then aUprefix = "Ammo Mode (Currently: Unlimited Ammo/Clip)"
      end

      GCCombox2.Items.clear()

      if Vtest == 0 or Vtest == -1 then
        Vtest = 0
        GCCombox2.items.add(aUprefix)
      else
        GCCombox2.items.add("Turn Off")
      end

      if Vtest == 1 then
        GCCombox2.items.add(aUprefix)
      else
        GCCombox2.items.add("Unlimited Ammo")
      end

      if Vtest == 2 then
        GCCombox2.items.add(aUprefix)
      else
        GCCombox2.items.add("Unlimited Clip")
      end

      if Vtest == 3 then
        GCCombox2.items.add(aUprefix)
      else
        GCCombox2.items.add("Unlimited Ammo/Clip")
      end

      GCCombox2.ItemIndex=Vtest
    end

    set_UnlimWPPrefix()

    GCCombox2.OnChange = function()
      local id = GCCombox2.ItemIndex

      if (id == 0) then
        Vtest = 0
        set_UnlimWPPrefix()
        do_ConfigSave(5, 0)
        aAmmoMode = 0
        check_unlimitedClip(-1)
      elseif (id == 1) then
        Vtest = 1
        set_UnlimWPPrefix()
        do_ConfigSave(5, 1)
        aAmmoMode = 1
      elseif (id == 2) then
        Vtest = 2
        set_UnlimWPPrefix()
        do_ConfigSave(5, 2)
        aAmmoMode = 2
      elseif (id == 3) then
        Vtest = 3
        set_UnlimWPPrefix()
        do_ConfigSave(5, 3)
        aAmmoMode = 3
      end
