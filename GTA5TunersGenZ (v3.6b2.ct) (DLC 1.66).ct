-- VenomKY's code bundle for Outfit Editor.

function _writeString(Address,String,Size)
  local bt=stringToByteTable(String)

  if #bt&gt;Size then
    return
  elseif #bt==Size then goto continue
  end

  for i=1,Size-(#bt),1 do
    bt[#bt+i]=0x00
  end

  ::continue::
  writeBytes(Address,bt)
end

function getSlotInfo(WhichSet,SlotID,SetID)
  local Buffer = {}

  if WhichSet == 1 then
    Buffer = {readInteger(GA(oWardrobeG+(0*oWPointA)+oWPointB+oWComponent+(SlotID*13)+SetID))
					,readInteger(GA(oWardrobeG+(0*oWPointA)+oWPointB+oWComponentTex+(SlotID*13)+SetID))}
