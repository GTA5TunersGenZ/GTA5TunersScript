local function FixVehicle()
  local Count,Count1 = 0,0
  local isDone = false
  local isBSTFound = false
  local FixVehValue = nil
  local BSTValue = nil
  local CPickupInterface = nil
  local PickupCount = nil
  local PickupList = nil

  ::reDropBST::
  SG('i',oVMYCar+894,1)
  sleep(1000)
  --SG('i',oNETTimeHelp+3689,5)

  FixVehValue = readQword("[PickupDataPTR]+pFixVeh")                 -- pFixVeh = 0x230
  BSTValue    = readQword("[PickupDataPTR]+pBST")                    -- pBST = 0x160

  if not FixVehValue or not BSTValue then goto Exit end

  ::reFix::
  writeFloat("[[[WorldPTR]+pCPed]+pCVehicle]+oVHealth", 999)     -- Set health to can pickup FixVeh

  CPickupInterface = getAddressSafe("[[ReplayInterfacePTR]+pCPickupInterface]") -- pCPickupInterface = 0x20
  if not CPickupInterface then goto Exit end

  PickupCount = readInteger(CPickupInterface + oPickupNum)    -- oPickupNum = 0x110
  PickupList = CPickupInterface + pPickupList                 -- pPickupList = 0x100

  if not PickupList then goto Exit end

  if PickupCount == 0 and not isDone then
    Count = Count +1

    if Count &gt; 10 then goto Exit end
    goto reDropBST
  end

  sleep(100)

  for i = 0,PickupCount do
    local p = readPointer(PickupList)+(i*0x10)

	if not p then goto continue end

	local PickupValue = readQword(readPointer(p) + pDroppedPickupData)         -- pDroppedPickupData = 0x470

	if not PickupValue then goto continue end

	if PickupValue == BSTValue then
	  isDone = false
      isBSTFound = true
	  writeQword(readPointer(p)+pDroppedPickupData,FixVehValue)

	  sleep(10)

	  local Vehx = readFloat "[[[WorldPTR]+pCPed]+pCVehicle]+oVPositionX"
	  local Vehy = readFloat "[[[WorldPTR]+pCPed]+pCVehicle]+oVPositionY"
	  local Vehz = readFloat "[[[WorldPTR]+pCPed]+pCVehicle]+oVPositionZ"

	  sleep(10)

	  writeFloat(readPointer(p)+oVPositionX, Vehx)
	  writeFloat(readPointer(p)+oVPositionY, Vehy)
	  writeFloat(readPointer(p)+oVPositionZ, Vehz)
	  writeFloat ("[[[WorldPTR]+pCPed]+pCVehicle]+oVDirt" ,0)     -- Wash Vehicle

      break
	end

    ::continue::
  end

  if not isBSTFound and Count &lt; 10 then
    Count = Count + 1
    goto reDropBST
  elseif not isDone and Count1 &lt; 3000 then -- about 5 min of waiting
    isDone = true
	Count1 = Count1 + 1
	goto reFix
  end

  ::Exit::

  if (GG('i',oNETTimeHelp+3690) ~= 0) then   -- Check if BST is enabled
    SG('i',oNETTimeHelp+3690, -1) -- Turn off BST if is enabled
  end

  FixVehicleThread = nil
end

if IsMeInVehicle() and FixVehicleThread == nil then FixVehicleThread = createNativeThread(FixVehicle) end

