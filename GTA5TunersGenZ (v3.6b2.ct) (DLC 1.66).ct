function AutoMenuS(number)
  local test = get_Config(1)

  if tonumber(number) == 0 then
    if test == 0 or test == 1 or test == 2 or test == 3 or test == 4 then
      return
    end
  end

  if AutoMenus == nil then
    local acprefix = nil
