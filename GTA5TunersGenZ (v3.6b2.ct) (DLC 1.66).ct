function check_scrTimers()
  sleep(250)

  local fs = nil
  local weGoodToLog = false
  local datestamp = ''
  local logBuff = ''

  if LUALOGs == 1 then
    fs = io.open(string.format('%s/GTA_GNullLuaLogs.ini',TrainerOrigin), "a");
    if fs ~= nil then
      weGoodToLog = true-- VenomKY's code bundle for Outfit Editor.
      datestamp = os.date("%I:%M %p %x")
    end
  end

  if ControlCenterTimer == 0 and sTATUsBoX then
    if aStatusThread and aStatusThread.Finished == true then
      if weGoodToLog then
        logBuff = '['..datestamp..']: Attempting to restart setStatuses/aStatusThread timer, check for errors running on this threaded timer.'
        fs:write(logBuff)
      end
      aStatusThread.terminate()
      aStatusThread=createNativeThread(setStatuses)
    end
  end

  if MyStatusThread and MyStatusThread.Finished == true then
    if weGoodToLog then
      logBuff = '['..datestamp..']: Attempting to restart myStatus/MyStatusThread timer, check for errors running on this threaded timer.'
      fs:write(logBuff)
    end
    MyStatusThread.terminate()
    MyStatusThread = createNativeThread(myStatus)
  end

  if autoAttachTimer and autoAttachTimer.Finished == true then
    if weGoodToLog then
      logBuff = '['..datestamp..']: Attempting to restart RunCheck/autoAttachTimer timer, check for errors running on this threaded timer.'
      fs:write(logBuff)
    end
    autoAttachTimer.terminate()
    autoAttachTimer = createNativeThread(RunCheck)
  end

  if fs then fs:close() end
  checkTimers  = nil
end

