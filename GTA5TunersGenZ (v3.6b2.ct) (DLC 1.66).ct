    newTELELIST.OnDblClick = function()
      if quickOnlineCheck() == false then return end
      local id = newTELELIST.ItemIndex + 1
      if id == 0 then return end
      local preINFO = nil

      if isTelePre == 0 then
        preINFO = tbl_telePRESETS[id][1]
        teleportTOPRESET(id)
        reset_TELEMenu(string.format([[Attempting to teleport to (%s) location. ]], preINFO))
        TELEMenuStatus.Lines.Text = string.format([[[Preset]: %s]], preINFO)
      elseif isTelePre == 1 then
         preINFO = tbl_teleCLICKS[id][1]
         if lastTeleReq ~= id then teleCommandCount = 1 end
         local togSpeed = get_teleKEYSpeed(-1)
         local x,y,z,head,head2 = getCurrentCoords()
         local distance = 4.5

         if togSpeed == 1 then
           distance = 15.5
         elseif togSpeed == 2 then
           distance = 40.5
         elseif togSpeed == 3 then
           distance = 100.5
         end

         if id == 1 then
           if x then
             x = x - (head2 * distance)
             y = y + (head * distance)
             teleport(x,y,z)
             TELEMenuStatus.Lines.Text = '[Forward]: '..string.format([[count: (%i) time(s).]], teleCommandCount)
           else
             TELEMenuStatus.Lines.Text = 'Error: Got bad x value, try again. Resetting (forward) attempt count!'
             teleCommandCount = 0
           end
         elseif id == 2 then
           if x then
             x = x + (head2 * distance)
             y = y - (head * distance)
             teleport(x,y,z)
             TELEMenuStatus.Lines.Text = '[Back] count: '..string.format([[(%i) time(s).]], teleCommandCount)
           else
             TELEMenuStatus.Lines.Text = 'Error: Got bad x value, try again. Resetting (back) attempt count!'
             teleCommandCount = 0
           end
         elseif id == 3 then
           if z then
             z = z + distance
             teleport(x,y,z)
             TELEMenuStatus.Lines.Text = '[Up]: count: '..string.format([[(%i) time(s).]], teleCommandCount)
           else
             TELEMenuStatus.Lines.Text = 'Error: Got bad z value, try again. Resetting (up) attempt count!'
             teleCommandCount = 0
           end
         elseif id == 4 then
           if z then
             z = z - distance
             teleport(x,y,z)
             TELEMenuStatus.Lines.Text = '[Down]: count: '..string.format([[(%i) time(s).]], teleCommandCount)
           else
             TELEMenuStatus.Lines.Text = 'Error: Got bad z value, try again. Resetting (down) attempt count!'
             teleCommandCount = 0
           end
         elseif id == 5 then
           if x then
             x = x + distance
             y = y - (distance * head2)
             teleport(x,y,z)
             TELEMenuStatus.Lines.Text = '[Left]: count: '..string.format([[(%i) time(s).]], teleCommandCount)
           else
             TELEMenuStatus.Lines.Text = 'Error: Got bad x value, try again. Resetting (left) attempt count!'
             teleCommandCount = 0
           end
         elseif id == 6 then
           if x then
             x = x - distance
             y = y + (distance * head2)
             teleport(x,y,z)
             TELEMenuStatus.Lines.Text = '[Right]: count: '..string.format([[(%i) time(s).]], teleCommandCount)
           else
             TELEMenuStatus.Lines.Text = 'Error: Got bad x value, try again. Resetting (right) attempt count!'
             teleCommandCount = 0
           end
         elseif id == 7 then
           local togNum,EStatus = get_teleKEYSpeed(0)
           TELEMenuStatus.Lines.Text = 'Easy Teleport speed set to: '..string.format([[(%s)]], EStatus)
         end
         lastTeleReq = id
         teleCommandCount = teleCommandCount + 1
      elseif isTelePre == 2 then
        preINFO = tbl_MyLocations[id][1]
        local xxx = tbl_MyLocations[id][2]
        local yyy = tbl_MyLocations[id][3]
        local zzz = tbl_MyLocations[id][4]
        teleport(xxx, yyy,  zzz)
        reset_TELEMenu(string.format([[Attempting to teleport to (%s) location. ]], preINFO))
      end
    end

    teleLOADPRE.OnClick = function() loadTELEPRESETS('Simple double-click on teleport location.') end

    teleCOMPACT.OnClick = function()
      if isTelePre == 1 then TELEMenuStatus.Lines.Text = 'Restoring form to compact view. '
      else reset_TELEMenu('Restoring form to compact view. ') end
      newTELEMENU.Height = 298
      newTELEMENU.Width = 446
      newTELETIT.Width = 215
      newTELELIST.Width = 215
    end

    teleTELEMAN.OnClick = function()
      if quickOnlineCheck() == false then return end
      if IsValidCoords(myX.Text, myY.Text, myZ.Text) == false then
        if isTelePre == 1 then TELEMenuStatus.Lines.Text = 'Error: You must use valid x, y, z numeric values!'
        else reset_TELEMenu('Error: You must use valid x, y, z numeric values! ') end
        return
      else
        if isTelePre == 1 then TELEMenuStatus.Lines.Text = 'Attempting to teleport to coordinates '..string.format([[(X: %s Y: %s Z: %s)]],myX.Text, myY.Text, myZ.Text)
        else reset_TELEMenu(string.format([[Attempting to teleport to coordinates (X: %s Y: %s Z: %s)]],myX.Text, myY.Text, myZ.Text)) end
        teleport(myX.Text, myY.Text, myZ.Text)
      end
    end

    newTELEMENU.OnResize = function()
      local w, h = newTELEMENU.Width,newTELEMENU.Height

      if h &gt; 298 then
        teleCOMPACT.top = 297
      else
        teleCOMPACT.top = 303
      end

      if h &lt; 298 then
        newTELEMENU.Height = 298
        newTELELIST.Height = 276
      else
        newTELELIST.Height = h - 22
      end

      if w &lt; 497 then
        newTELEMENU.Width = 446
        newTELETIT.Width = 215
        newTELELIST.Width = 215
      else
        newTELETIT.Width = w - 231
        newTELELIST.Width = w - 231
      end

      TeleMenuInfo.Left = w - 229
      TELEMenuStatus.Left = w - 229
      teleLOCSELDEL.Left = w - 230
      teleADDCURLOC.Left = w - 230
      teleADDNEWLOC.Left = w - 230
      teleMODLOC.Left = w - 116
      tpModel.Left = w - 229
      myX.Left = w - 229
      myY.Left = w - 153
      myZ.Left = w - 76
      teleLOADPRE.Left = w - 230
      teleLOADMY.Left = w - 230
      teleCOMPACT.Left = w - 230
      teleTELEMAN.Left = w - 230
      teleEASYCLICK.Left = w - 230
    end

    newTELEMENU.OnClose = function ()
      newTELEMENU.destroy()
      tbl_MyLocations={}
      newTELEMENU=nil
      newTELECAT=nil
      newTELELIST=nil
      TeleMenuInfo=nil
      TELEMenuStatus=nil
      teleMODLOC=nil
      tpModel=nil
      myX=nil
      myY=nil
      myZ=nil
      teleLOADPRE=nil
      teleLOADMY=nil
      teleCOMPACT=nil
      teleTELEMAN=nil
      teleEASYCLICK=nil
      if myCoordTimer ~= nil then myCoordTimer.destroy() end
      if myTempTimer ~= nil then myTempTimer.destroy() end
      myCoordTimer = nil
      myTempTimer = nil
    end
  end
end

-- Get Current Vehicle hash

function getCurrentVeh()
  local modelptr = readPointer('[[[WorldPTR]+pCPed]+pCVehicle]+pCModelInfo')
  if not modelptr then return false end
  local hash = readInteger(modelptr + oModelHash)
  return hash
end

