    if not p or p == nil then goto continue end

    local lc_p,lc_n=readInteger(p+0xB0),readString(p+0xD4)

    if not lc_n or lc_n == nil then goto continue
    elseif string.lower(lc_n) == string.lower(Name) and lc_p ~= 0 then return p+0xB0
    end

    ::continue::
   end

   return false
end

