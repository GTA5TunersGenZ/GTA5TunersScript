     aStatusThread = nil
   elseif aStatusThread ~= nil then aStatusThread.Enabled = false
   end
   tbl_checkBoxes = { }
   tbl_LiveStatus = { }
   StatusMemo = nil
   StatusMemo2 = nil
   StatusMemo3 = nil
   StatusMemo4 = nil
   StatusMemo5 = nil
   StatusMemo6 = nil
   StatusMemo7 = nil
   sTATUsBoX.destroy()
   sTATUsBoX = nil
   return
  end

  sTATUsBoX.show()
end

function setStatuses(_thread)
  if ControlCenterTimer == 0 then _thread.freeOnTerminate(false) end

  while (sTATUsBoX) and (ControlCenterTimer == 1) or (ControlCenterTimer == 0) and (sTATUsBoX) and (not _thread.Terminated) do
    if is_TableEmpty(tbl_LiveStatus) then
