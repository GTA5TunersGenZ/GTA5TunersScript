function getAddresses(symbol)
  if not symbol then
    print("WorldPTR: %x",readPointer('WorldPTR'))
    print("BlipPTR: %x",readPointer('BlipPTR'))
    print("GlobalPTR: %x",readPointer('GlobalPTR'))
    print("ReplayInterfacePTR: %x",readPointer('ReplayInterfacePTR'))
    print("LocalScriptsPTR: %x",readPointer('LocalScriptsPTR'))
    print("PlayerCountPTR: %x",readPointer('PlayerCountPTR'))
    print("SettingsPTRs: %x",readPointer('SettingsPTRs'))
    print("PickupDataPTR: %x",readPointer('PickupDataPTR'))
    print("WeatherADDR: %x",readPointer('WeatherADDR'))
    print("FriendlistPTR: %x",readPointer('FriendlistPTR'))
