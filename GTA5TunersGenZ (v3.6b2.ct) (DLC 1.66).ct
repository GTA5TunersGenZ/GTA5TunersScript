      protMenuStatus.Left = w - 229
      loadReset.Left = w - 75
    end

    protectionForm.OnClose = function ()
      protectionForm.destroy()
      protectionForm=nil
      protectionList=nil
      protMenuInfo=nil
    end

    loadButton.OnClick = function()
      reset_PROTMenu()

      local fsSS = io.open(string.format('%s/GTA_GNullProtections.ini',TrainerOrigin), "r");
      local fileInfo = string.format('%sGTA_GNullProtections.ini',TrainerOrigin)

      if fsSS == nil then
        local iNFOBuffer = string.format('Error opening file: (%s). No settings saved? Read Error?', fileInfo)
        protMenuStatus.Lines.Text = iNFOBuffer
        return
      end

      tbl_ReloadEvents = { }
      local iGotEvents = 0

      local iNFOBuffer = string.format('Loading protection settings from file (%s)...', fileInfo)
      protMenuStatus.Lines.Text = iNFOBuffer

      for c=1,120,1 do
        local line=fsSS:read("*line")
        if line == nil or line == '' then goto continue end
        local tst1,tst2 = string.find(line,':')
        if not tst1 or not tst2 then goto continue end
        local sValue = string.sub(line,tst1+1,-1)
        local sString = string.sub(line,1,tst1-1)
        if not sValue or not sString then goto continue end

        table.insert(tbl_ReloadEvents, c, {sString, sValue})
        iGotEvents = 1

        ::continue::
      end

      if iGotEvents == 1 then
        f_PrintScriptEvent('reload')
      else
        local iNFOBuffer = string.format('No valid protections settings loaded from file (%s)..', fileInfo)
        protMenuStatus.Lines.Text = iNFOBuffer
      end
    end

    loadFalse.OnClick = function()
      reset_PROTMenu()
      local iNFOBuffer = string.format('Resetting all protections to FALSE/OFF.')
      protMenuStatus.Lines.Text = iNFOBuffer
      f_PrintScriptEvent('allfalse')
    end

    loadReset.OnClick = function()
      reset_PROTMenu()
      local fileInfo = string.format('%sGTA_GNullProtections.ini',TrainerOrigin)
      os.remove(fileInfo)
      local iNFOBuffer = string.format('Removed Protection Settings Save File (if exists): (%s)...', fileInfo)
      protMenuStatus.Lines.Text = iNFOBuffer
      f_PrintScriptEvent('reset')
    end

    protectionList.OnClick = function()
      local index = protectionList.ItemIndex + 1
      if index == 0 then return end
      f_PrintScriptEvent(index)
      protMenuStatus.Lines.Text = ''
    end

    protectionList.OnDblClick = function()
      local index = protectionList.ItemIndex + 1
      if index == 0 then return end
      v_ProtectEvent(index)
    end
  end
end

