    local function do_AddXYZ(thePurpose, theID, tpName, XCoord, YCoord, ZCoord)
      if is_TableEmpty(tbl_MyLocations) == true then
        check_MyCoordTimer(0)
        getAndSaveTele(1)
      end

      if thePurpose == 'addnew' then
        if checkDup(tpName, XCoord, YCoord, ZCoord) == true then
          TELEMenuStatus.Lines.Text = 'This teleport location has already been added to your list.'
          return
        end

        TELEMenuStatus.Lines.Text = 'Added new location: '..string.format([[(%s) to the teleport list.]], tpModel.Text)

        tpModel.Text = tpName
        myX.Text = XCoord
        myY.Text = YCoord
        myZ.Text = ZCoord

        table.insert(tbl_MyLocations, {tpName, XCoord, YCoord, ZCoord})
        do_aDDRemove(-1, 0)
        return
      end

      if thePurpose == 'modify' and theID ~= 0 then
        if checkDup(tpName, XCoord, YCoord, ZCoord) == true then
          TELEMenuStatus.Lines.Text = 'Teleport location: '..string.format([[(%s) already has exact values, no changes made.]], tpName)
          return
        end

        tbl_MyLocations[theID][1] = tpName
        tbl_MyLocations[theID][2] = XCoord
        tbl_MyLocations[theID][3] = YCoord
        tbl_MyLocations[theID][4] = ZCoord

        TELEMenuStatus.Lines.Text = 'Location: '..string.format([[(%s) has been modified.]], tpModel.Text)
        do_aDDRemove(-1, 0)
      end
    end

