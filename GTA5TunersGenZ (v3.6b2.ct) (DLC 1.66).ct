  if quickOnlineCheck() == false then
    nClubAutoPop = 0
	memoryrecord_unfreeze(memoryrec)
    return false
  end

  if tbl_BlockMyMessage[17] ~= 1 then
    myShowMessage('Automated NightClub Popularity Activated!','Automated NightClub Popularity Activated!', 5000)
    tbl_BlockMyMessage[17] = 1
  end

  return true
end

local StopITGod = getAddressList().getMemoryRecordByID(tonumber(ANightCTNum))
memoryrecord_onActivate(StopITGod, stopActivate)

nClubAutoPop = 1

function NightCLBPOP()
   local myNCCounter = 0
   setStat('CLUB_POPULARITY',1000)

   while 1 == 1 do
      if ISGTARunning() == false then
        nClubAutoPop = 0
        if aNightNCThread then aNightNCThread.terminate() end
        aNightNCThread = nil
        do_tableX(ANightCTNum, 0, 'GNull', 1)
        break
      end

      local contest = getAddressSafe('WorldPTR')

      if not contest then
        sleep(10000)
        goto blah
      end

      if nClubAutoPop == 0 then
        if aNightNCThread then aNightNCThread.terminate() end
        aNightNCThread = nil
        do_tableX(ANightCTNum, 0, 'GNull', 1)
        break
      elseif tonumber(myNCCounter) &gt;= 160 and nClubAutoPop == 1 then
        setStat('CLUB_POPULARITY',1000)
        myNCCounter = 0
      end

      if nClubAutoPop == 1 then do_tableX(ANightCTNum, 1, 'GNull', 1) end

      myNCCounter = myNCCounter + 1

      sleep (5000)

     ::blah::
   end
end

if aNightNCThread == nil then aNightNCThread=createNativeThread(NightCLBPOP) end

