    local function reset_PROTMenu()
      protMenuInfo.Lines.Text = string.format([[Double Click on Event to Toggle Protection (Enabled/Disable)!

It will AutoSave TRUE statements!
]])
    end

    reset_PROTMenu()

    local function f_GetScriptEvents()
      autoAssemble([[
       aobscanmodule(ScriptEventPTR,GTA5.exe,48 83 EC 28 E8 ? ? ? ? 48 8B 0D ? ? ? ? 4C 8D 0D ? ? ? ? 4C 8D 05 ? ? ? ? BA 03)
       registerSymbol(ScriptEventPTR)
       ]])

      local event_addr=getAddressSafe("ScriptEventPTR")+16;
      local ic,_dis1,_dis2,_is1,_is2=1,'','',0,0
      local function lf_Title(first,rest) return first:upper()..rest:lower()end

      local tbl_orGANIze = { }
      tbl_ScriptEvents = { }

      for i=-1,100 do
        _dis1=disassemble(event_addr+(i*31))
        _dis2=disassemble((event_addr+7)+(i*31))
        _is1=string.find(_dis1,"lea r9,")
        _is2=string.find(_dis2,"lea r8,")

        if _is1~=nil and _is2~=nil then
          local _status,l1,l2=false,0,0
          local _address,_bytes,_opcode,_extra='','','',''
          _address,_bytes,_opcode,_extra=splitDisassembledString(_dis1)

          local _EventString=string.gsub(_address,"(%a)([%w']*)",lf_Title)

          if _address == nil then return end

          _EventString=string.gsub(_EventString,"_"," ")
          _EventString=string.sub(_EventString,3,string.len(_EventString)-2)
          _EventString=string.gsub(_EventString," Event", "")
          _address,_bytes,_opcode,_extra=splitDisassembledString(_dis2)
          l1=string.find(_bytes,"lea r8,")
          l2=string.len(_bytes)
          local _EventAddress=string.sub(_bytes,l1+8,l2-1)

          if _EventAddress == nil then return end

          if readBytes(_EventAddress)==0x48 then _status=false
          elseif readBytes(_EventAddress)==0xC3 then _status=true
          else return end
          tbl_orGANIze[ic]={_EventAddress,_EventString,_status}
          ic=ic+1
        end
      end

      if is_TableEmpty(tbl_orGANIze) == false then
        table.sort(tbl_orGANIze, function(a, b) return a[2] &lt; b[2] end)

        for k, v in pairs(tbl_orGANIze) do
          tbl_ScriptEvents[k]={v[1],v[2],v[3]}
        end
      end
    end

    -- Usage: [[//f_PrintScriptEvent(7) or f_PrintScriptEvent('all')]]

    local function f_PrintScriptEvent(ID)
      if ID == nil then return end

      if string.lower(tostring(ID))=='all' or string.lower(tostring(ID))=='write' or string.lower(tostring(ID))=='reset' or string.lower(tostring(ID))=='reload' or string.lower(tostring(ID))=='allfalse' then
        protectionList.Items.clear()

        local fileInfo = string.format('%sGTA_GNullProtections.ini',TrainerOrigin)
        local wRTFile = nil
        local gotTruth = 0
        local doReset = 0
        local doReload = 0
        local doAllFalse = 0
        local doFalseCnt = 0

        if string.lower(tostring(ID)) == 'reset' then doReset = 1 end
        if string.lower(tostring(ID))=='reload' then doReload = 1 end
        if string.lower(tostring(ID))=='allfalse' then doAllFalse = 1 end

        if string.lower(tostring(ID)) == 'write' then
          wRTFile = io.open(string.format('%s/GTA_GNullProtections.ini',TrainerOrigin), "w+")

          if wRTFile == nil then
            local iNFOBuffer = string.format('Error writing to file: (%s). Permissions error?', fileInfo)
            protMenuStatus.Lines.Text = iNFOBuffer
          else
            local iNFOBuffer = string.format('Saving to (%s)...', fileInfo)
            protMenuStatus.Lines.Text = iNFOBuffer
          end
        end

        local ssTESTCnt = 0

        for i = 1,81 do
          local tbl_SE,s={},''
          tbl_SE=tbl_ScriptEvents[i]

          if tbl_SE ~= nil then
            local ev_addr=tbl_SE[1]

            if doReload == 1 then
              for k, v in pairs(tbl_ReloadEvents) do
                if v[1]:lower() == tbl_SE[2]:lower() then
                  if v[2]:lower() == 'true' then
                    writeBytes(tbl_SE[1],0xC3)
                    tbl_ScriptEvents[i][3]=true;
                    ssTESTCnt = ssTESTCnt + 1
                  elseif v[2]:lower() == 'false' then
                    writeBytes(tbl_SE[1],0x48)
                    tbl_ScriptEvents[i][3]=false;
                    ssTESTCnt = ssTESTCnt + 1
                  end
                end
              end
            end

            if doReset == 1 or doAllFalse == 1 then
              if readBytes(tbl_SE[1]) == 0xC3 then
                writeBytes(tbl_SE[1],0x48)
                tbl_ScriptEvents[i][3]=false
                doFalseCnt = doFalseCnt + 1
              end
            end

            if readBytes(ev_addr)==0x48 then s='false'
            elseif readBytes(ev_addr)==0xC3 then s='true'
            else goto continue;
            end

            local addEventStr = nil

            if s:upper() == 'TRUE' then
              addEventStr  = string.format('[%i] Event: (%s) Protected: [%s]',i,tbl_SE[2],s:upper())
            else
              addEventStr  = string.format('[%i] Event: (%s) Protected: (%s)',i,tbl_SE[2],s:upper())
            end

            protectionList.Items.add(addEventStr)

            if doReset == 0 and s:upper() == 'TRUE' and wRTFile and string.lower(tostring(ID)) == 'write' then
              gotTruth = 1
              local eventName = string.format('%s:%s', tbl_SE[2], s:upper())
              wRTFile:write(eventName,"\n")
            end
          end
          ::continue::
        end

        if doReload == 1 then
          if ssTESTCnt &gt; 0 then
            local iNFOBuffer = string.format('Reloaded (%i) saved protection setting(s) from file: (%s).', ssTESTCnt, fileInfo)
            protMenuStatus.Lines.Text = iNFOBuffer
          else
            local iNFOBuffer = string.format('No valid protections settings loaded from file (%s)..', fileInfo)
            protMenuStatus.Lines.Text = iNFOBuffer
          end
        end

        if doAllFalse == 1 then
          local iNFOBuffer = string.format('All protection settings already set to FALSE/OFF.')

          if doFalseCnt &gt; 0 then
            iNFOBuffer = string.format('Set a Total of (%i) protection(s)/setting(s) to FALSE/OFF. Restore them by clicking "Load Previous" button.', doFalseCnt)
          end

          protMenuStatus.Lines.Text = iNFOBuffer
          reset_PROTMenu()
        end

        if wRTFile then
          wRTFile:close()

          if gotTruth == 0 then
            os.remove(fileInfo)
            local iNFOBuffer = string.format('All values are FALSE, removing protection settings file: (%s)...', fileInfo)
            protMenuStatus.Lines.Text = iNFOBuffer
            reset_PROTMenu()
          end
        end
      else
       local tbl_SE,s={},''
       tbl_SE=tbl_ScriptEvents[ID]

       if tbl_SE ~= nil then
         local ev_addr=tbl_SE[1]

         if readBytes(ev_addr)==0x48 then s='false'
         elseif readBytes(ev_addr)==0xC3 then s='true'
         else goto continue; end

         protMenuInfo.Lines.Text = string.format('[Event %i]: (%s)\n[Protected]: (%s)\n[Address]: (0x%s)\n\nDouble Click to Enable/Disable.',ID,tbl_SE[2],s:upper(),tbl_SE[1])
       end
       ::continue::
      end
    end

    f_GetScriptEvents()
    f_PrintScriptEvent('all')

    local function v_ProtectEvent(ID)
      if is_TableEmpty(tbl_ScriptEvents) then return end

      local tbl_SE = tbl_ScriptEvents[ID]

      if tbl_SE == nil then return end

      if readBytes(tbl_SE[1]) == 0x48 then
        writeBytes(tbl_SE[1],0xC3)
        tbl_ScriptEvents[ID][3]=true;
      elseif readBytes(tbl_SE[1]) == 0xC3 then
        writeBytes(tbl_SE[1],0x48)
        tbl_ScriptEvents[ID][3]=false;
      else
        protMenuInfo.Lines.Text = string.format('ERROR: Wrong Input.')
        protMenuStatus.Lines.Text = ''
        return
      end

      f_PrintScriptEvent(ID)
      f_PrintScriptEvent('write')

      protectionList.ItemIndex = ID - 1
    end

