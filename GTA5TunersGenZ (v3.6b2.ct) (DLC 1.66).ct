  SG('i',oVMCreate+27+8, -1)
  SG('i',oVMCreate+27+19, 4)
  SG('i',oVMCreate+27+21, 4)  -- Engine (0-3)
  SG('i',oVMCreate+27+22, 3)
  SG('i',oVMCreate+27+23, 3)  -- Transmission (0-9)
  SG('i',oVMCreate+27+24, 58)
  SG('i',oVMCreate+27+26, 5)  -- Armor (0-18)
  SG('i',oVMCreate+27+27, 1)  -- Turbo (0-1)
  SG('i',oVMCreate+27+65, 2) -- Window tint 0-6
  SG('i',oVMCreate+27+69, -1) -- Wheel type
  SG('i',oVMCreate+27+33, -1) -- Wheel Selection
  SG('i',oVMCreate+27+25, 8)  -- Suspension (0-13)
  SG('i',oVMCreate+27+19, -1)
  writeBytes(GA(oVMCreate+27+77)+1, 2) -- 2:bulletproof 0:false

  local wp,wp2 = 2,1

  if model == 'oppressor2' then
    wp = 2
  elseif model == 'apc' or model == 'deluxo' then
    wp = -1
  elseif model == 'bombushka' then
    wp = 1
  elseif model == 'tampa3' or model == 'insurgent3' or model == 'halftrack' then
    wp = 3
  elseif model == 'barrage' then
    wp = 30
  end

  SG('i',oVMCreate+27+15, wp)  -- primary weapon
  SG('i',oVMCreate+27+20, wp2) -- secondary weapon
  --SG('s',oVMCreate+27+1,"3lit3") -- License plate
end

function SpawnMyCar(value)
  GetVehicleSlots()

  for k,v in pairs(tbl_GSV) do
    if v[2]:upper() == value:upper() or v[4]:upper() == value:upper() then
      SG('i',oVMYCar+986,tbl_GSV[k][1])
      SG('i',oVMYCar+983,1)
      Sleep(700)
      SG('i',oVGETIn+8,1)
      break
    end
  end
end

function SpawnCar(value)
  for k,v in pairs(tbl_Vehicles) do
    if v[1]:upper() == value:upper() then
      local ped_pos_x = getAddressSafe("[[[WorldPTR]+8]+30]+50")
      local ped_pos_y = getAddressSafe("[[[WorldPTR]+8]+30]+54")
      local ped_pos_z = getAddressSafe("[[[WorldPTR]+8]+30]+58")
      MYCREATE_VEHICLE(tbl_Vehicles[k][1],ped_pos_x,ped_pos_y,ped_pos_z,0)
      break
    end
  end
end

