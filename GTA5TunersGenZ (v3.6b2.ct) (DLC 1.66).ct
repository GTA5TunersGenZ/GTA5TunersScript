function StartStatusLoop()
  if sTATUsBoX == nil then CreateStatusMenu() end

  if ControlCenterTimer == 0 then sleep(20) end

  if not sTATUsBoX then
    if ControlCenterTimer == 0 then if aStatusThread ~= nil and aStatusThread.Finished == false then StatusThread.terminate() end
    elseif aStatusThread ~= nil then aStatusThread.Enabled = false
    end
    return
  end

  if ControlCenterTimer == 1 then
    setStatuses()

    if aStatusThread == nil then
      aStatusThread = createTimer(getMainForm())
      aStatusThread.Interval = 400
      aStatusThread.OnTimer = setStatuses
      aStatusThread.Enabled = true
    else aStatusThread.Enabled = true
    end

    return
  end

  local startStatus = false

  if aStatusThread == nil then startStatus = true
  elseif aStatusThread ~= nil and aStatusThread.Finished == true then
    aStatusThread.terminate()
    startStatus = true
  end

  if startStatus == true then aStatusThread = createNativeThread(setStatuses) end
end

