function quickVehAdd()
  if IsMeInVehicle() then
    if not getAddressSafe("[[[[WorldPTR]+pCPed]+pCVehicle]+pCModelInfo]+oModelHash") then return false end

    local theModel = readInteger("[[[[WorldPTR]+pCPed]+pCVehicle]+pCModelInfo]+oModelHash")

    if theModel then
      if not is_TableEmpty(tbl_vehicleSaveSettings) then
        for k, v in pairs(tbl_vehicleSaveSettings) do if theModel == v[1] then return true end end
      end

      local brkForce = readFloat("[[[[WorldPTR]+pCPed]+pCVehicle]+pCHandlingData]+oBrakeForce")
      local hbrkForce = readFloat("[[[[WorldPTR]+pCPed]+pCVehicle]+pCHandlingData]+oHandbrakeForce")
      local massForce = readFloat("[[[[WorldPTR]+pCPed]+pCVehicle]+pCHandlingData]+oMass")
      local TrackMaxForce = readFloat("[[[[WorldPTR]+pCPed]+pCVehicle]+pCHandlingData]+oTractionCurveMax")
      local TrackMinForce = readFloat("[[[[WorldPTR]+pCPed]+pCVehicle]+pCHandlingData]+oTractionCurveMin")
      local AccelForce = readFloat("[[[[WorldPTR]+pCPed]+pCVehicle]+pCHandlingData]+oAcceleration")
      local dirtForce =  readFloat("[[[WorldPTR]+pCPed]+pCVehicle]+oVDirt")
      local theGravity = readFloat("[[[WorldPTR]+pCPed]+pCVehicle]+oVGravity")

      table.insert(tbl_vehicleSaveSettings, {theModel, brkForce, hbrkForce, massForce, TrackMaxForce, TrackMinForce, AccelForce, dirtForce, theGravity})
      return true
    end
  end

  return false
end

function QuickCarSet()
  if quickCarToggle == 0 then
    quickCarTicker = quickCarTicker + 1

    if quickCarTicker &gt; 4 then
      if quickCarTimer ~= nil then quickCarTimer.Enabled = false end
      quickCarTicker = 0
      return
    end
    return
  end

  quickCarTicker = 0

  local function Vehreset_it() do_tableX(AVehExtTNum, 0, 0000, 1) end

  if not IsMeInVehicle() or not quickVehAdd() then
    Vehreset_it()
    return
  end

  local brkForce = "[[[[WorldPTR]+pCPed]+pCVehicle]+pCHandlingData]+oBrakeForce"
  local hbrkForce = "[[[[WorldPTR]+pCPed]+pCVehicle]+pCHandlingData]+oHandbrakeForce"
  local massForce = "[[[[WorldPTR]+pCPed]+pCVehicle]+pCHandlingData]+oMass"
  local TrackMaxForce = "[[[[WorldPTR]+pCPed]+pCVehicle]+pCHandlingData]+oTractionCurveMax"
  local TrackMinForce = "[[[[WorldPTR]+pCPed]+pCVehicle]+pCHandlingData]+oTractionCurveMin"
  local AccelForce = "[[[[WorldPTR]+pCPed]+pCVehicle]+pCHandlingData]+oAcceleration"
  local dirtForce =  "[[[WorldPTR]+pCPed]+pCVehicle]+oVDirt"
  local theGravity = "[[[WorldPTR]+pCPed]+pCVehicle]+oVGravity"

  local brkValue = isAddressSafe(brkForce, 4)
  local hbrValue = isAddressSafe(hbrkForce, 4)
  local massValue = isAddressSafe(massForce, 4)
  local TrackMaxValue = isAddressSafe(TrackMaxForce, 4)
  local TrackMinValue = isAddressSafe(TrackMinForce, 4)
  local AccelValue = isAddressSafe(AccelForce, 4)
  local dirtValue = isAddressSafe(dirtForce, 4)
  local GravityValue = isAddressSafe(theGravity, 4)

  if not brkValue or not hbrValue or not massValue or not TrackMaxValue or not TrackMinValue or not AccelValue or not dirtValue or not GravityValue then return end

  if quickCarToggle == 1 then
    if brkValue ~= 2.7 then writeFloat(brkForce, 2.7) end
    if hbrValue ~= 2.4 then writeFloat(hbrkForce, 2.4) end
    if TrackMaxValue ~= 2.849999905 then writeFloat(TrackMaxForce, 2.849999905) end
    if TrackMinValue ~= 2.849999905 then writeFloat(TrackMinForce, 2.849999905) end
    if AccelValue ~= 1 then writeFloat(AccelForce, 1) end
    if dirtValue ~= 2 then writeFloat(dirtForce, 2) end
    if GravityValue ~= 13.0 then writeFloat(theGravity, 13.0) end
  elseif quickCarToggle == 2 then
    do_tableX(AVehExtTNum, 1, 0060, 1)
    if brkValue ~= 2.7 then writeFloat(brkForce, 2.7) end
    if hbrValue ~= 2.4 then writeFloat(hbrkForce, 2.4) end
    if massValue ~= 1000000000 then writeFloat(massForce, 1000000000) end
    if TrackMaxValue ~= 2.849999905 then writeFloat(TrackMaxForce, 2.849999905) end
    if TrackMinValue ~= 2.849999905 then writeFloat(TrackMinForce, 2.849999905) end
    if AccelValue ~= 1 then writeFloat(AccelForce, 1) end
    if dirtValue ~= 2 then writeFloat(dirtForce, 2) end
    if GravityValue ~= 13.0 then writeFloat(theGravity, 13.0) end
  elseif quickCarToggle == 3 then
    do_tableX(AVehExtTNum, 1, 0360, 1)
    if brkValue ~= 2.7 then writeFloat(brkForce, 2.7) end
    if hbrValue ~= 2.4 then writeFloat(hbrkForce, 2.4) end
    if massValue ~= 1000000000 then writeFloat(massForce, 1000000000) end
    if TrackMaxValue ~= 2.849999905 then writeFloat(TrackMaxForce, 2.849999905) end
    if TrackMinValue ~= 2.849999905 then writeFloat(TrackMinForce, 2.849999905) end
    if AccelValue ~= 1 then writeFloat(AccelForce, 1) end
    if dirtValue ~= 2 then writeFloat(dirtForce, 2) end
    if GravityValue ~= 13.0 then writeFloat(theGravity, 13.0) end
  elseif quickCarToggle == 4 then
    do_tableX(AVehExtTNum, 1, 0360, 1)
    if brkValue ~= 5.5 then writeFloat(brkForce, 5.5) end
    if hbrValue ~= 5.5 then writeFloat(hbrkForce, 5.5) end
    if massValue ~= 1000000000 then writeFloat(massForce, 1000000000) end
    if TrackMaxValue ~= 5.649999905 then writeFloat(TrackMaxForce, 5.649999905) end
    if TrackMinValue ~= 5.649999905 then writeFloat(TrackMinForce, 5.649999905) end
    if AccelValue ~= 12.5 then writeFloat(AccelForce, 12.5) end
    if dirtValue ~= 2 then writeFloat(dirtForce, 2) end
    if GravityValue ~= 13.5 then writeFloat(theGravity, 13.5) end
  elseif quickCarToggle == 5 then
    quickCarToggle = 0
    local theModel = readInteger("[[[[WorldPTR]+pCPed]+pCVehicle]+pCModelInfo]+oModelHash")
    for k, v in pairs(tbl_vehicleSaveSettings) do
      if theModel == v[1] then
        local carSet = { }
        carSet[1] = joaat("Toreador")
        carSet[2] = joaat("Starling")
        carSet[3] = joaat("Oppressor2")
        carSet[4] = joaat("Voltic2")
        carSet[5] = joaat("Vigilante")
        carSet[6] = joaat("Oppressor")
        carSet[7] = joaat("Scramjet")
        carSet[8] = joaat("Ruiner2")

        if theModel == carSet[1] or theModel == carSet[2] or theModel == carSet[3] or theModel == carSet[4] then
          do_tableX(AVehExtTNum, 0, 0040, 1) -- Toreader, Starling, Oppressor2 &amp; Voltic Reset
        elseif theModel == carSet[5] or theModel == carSet[6] then
          do_tableX(AVehExtTNum, 0, 0042, 1) -- Vigilante &amp; Oppressor1 Reset
        elseif theModel == carSet[7] then
          do_tableX(AVehExtTNum, 0, 0060, 1) -- ScramJet Reset
        elseif theModel == carSet[8] then
          do_tableX(AVehExtTNum, 0, 0120, 1) -- Ruiner jump and parachute.
        else
          do_tableX(AVehExtTNum, 0, 0000, 1) -- All non-boost cars.
        end

        writeFloat("[[[[WorldPTR]+pCPed]+pCVehicle]+pCHandlingData]+oBrakeForce", v[2])
        writeFloat("[[[[WorldPTR]+pCPed]+pCVehicle]+pCHandlingData]+oHandbrakeForce", v[3])
        writeFloat("[[[[WorldPTR]+pCPed]+pCVehicle]+pCHandlingData]+oMass", v[4])
        writeFloat("[[[[WorldPTR]+pCPed]+pCVehicle]+pCHandlingData]+oTractionCurveMax", v[5])
        writeFloat("[[[[WorldPTR]+pCPed]+pCVehicle]+pCHandlingData]+oTractionCurveMin", v[6])
        writeFloat("[[[[WorldPTR]+pCPed]+pCVehicle]+pCHandlingData]+oAcceleration", v[7])
        writeFloat("[[[WorldPTR]+pCPed]+pCVehicle]+oVDirt", v[8])
        writeFloat("[[[WorldPTR]+pCPed]+pCVehicle]+oVGravity", v[9])

        break
      end
    end
  end
end

