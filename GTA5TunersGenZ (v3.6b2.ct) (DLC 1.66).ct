-- RunCheck Start On Thread Timer.

function RunCheck(_thread)
  _thread.freeOnTerminate(false)
  local lockSession = false
  local runLoadOnce = false
  local delaySession = false
  local autoruncount = 0
  local adelay = 0
  local gTest

  while (not _thread.Terminated)  do
    if not pCheckProcess('GTA5.exe') then
      myCaption = 0
      if lockSession then synchronize(function() check_memoryRecords() end)unregallsyms() end
      lockSession = false
      runLoadOnce = false
      delaySession = false
      addressCheck = false
      autoruncount = 0

      if adelay &lt; 8 then adelay = adelay + 1 end

      goto continue
    end

    gTest = getAddressSafe('GlobalPTR')

    if gTest then
      if aConnect == 1 or aConnect == 4 then
        if readInteger("SettingsPTR+oStartupFlow") == 1 then writeInteger("SettingsPTR+oStartupFlow", 0) end
	    if readInteger("SettingsPTR+oGTAOnline") == 1 then writeInteger("SettingsPTR+oGTAOnline", 0) end
      elseif aConnect == 2 then
        if readInteger("SettingsPTR+oStartupFlow") == 1 then writeInteger("SettingsPTR+oStartupFlow", 0) end
	    if readInteger("SettingsPTR+oGTAOnline") == 0 then writeInteger("SettingsPTR+oGTAOnline", 1) end
      elseif aConnect == 3 and readInteger("SettingsPTR+oStartupFlow") == 0 then writeInteger("SettingsPTR+oStartupFlow", 1)
      end
    end

    if not lockSession then
      if not gTest then
        myCaption = 1
        startScriptLoad(1)
        goto continue
      else
        local SessionState = get_ConStatus()

        if SessionState == -1 then
          myCaption = 2
          startScriptLoad(2)
          goto continue
        end

        if not runLoadOnce then
          checkScriptload()
          runLoadOnce = true
        end

        if adelay &lt; 2 then
          adelay = 0
          myCaption = 3

          if SessionState == 0 then myCaption = 4
          elseif SessionState == 4 then myCaption = 5
          elseif SessionState == 10 then myCaption = 6
          end

          if SessionState ~= 4 then synchronize(function() check_unlimitedClip(-2) end) end

          lockSession = true

          goto continue
        end

        local taa = get_Config(1)

        if taa ~= 0 and taa ~= 3 then
          local tstatus = nil

          if taa == 1 then tstatus = 7
          elseif taa == 4 then tstatus = 9
          elseif taa == 2 then tstatus = 8
          elseif taa ~= 1 and taa ~= 2 and taa ~= 4 then tstatus = 7
          end

          if SessionState == 0 then
            if not delaySession then
              delaySession = true
              loadit()
            elseif delaySession then
              delaySession = false
              if tstatus == nil then myCaption = 6
              else myCaption = tstatus
              end
            end
          elseif SessionState == 4 then
            lockSession = true
            myCaption = 5
          end
        else
          myCaption = 10
          lockSession = true
        end
      end
    elseif lockSession then
      if not getAddressSafe('WorldPTR') then goto continue end

      local SessionState = get_ConStatus()

      if SessionState ~= 4 then synchronize(function() check_unlimitedClip(-2) end) end
      if SessionState == -1 then myCaption = 2
      elseif SessionState == 0 then myCaption = 4
      elseif SessionState == 4 then
        myCaption = 5

        if autoruncount &gt; 4 then
          synchronize(function() check_memoryRecords() end)

          if aFKtoggle == 1 then run_antiIdle(1) end

          if player_tracking == 1 then turnOnOffGPS(0)
          else turnOnOffGPS(1)
          end

          synchronize(function() check_unlimitedClip() end)

          local opCoolDown = tonumber(GA(OPRCoolDown))
          if opCoolDown ~= 1500 then SG('i',OPRCoolDown,1500) end

          autoruncount = 0
        else autoruncount = autoruncount + 1
        end
      elseif SessionState == 10 then myCaption = 6
      end
    end

    ::continue::

    sleep(750)
  end
end

