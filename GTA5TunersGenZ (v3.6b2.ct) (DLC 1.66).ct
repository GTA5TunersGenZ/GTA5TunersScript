      elseif firstReset == 1 or firstReset == 2 then
        newFace = "(}:op)"
        firstReset = 3
      elseif firstReset == 3 then
        newFace = "({:oD)"
        firstReset = 4
      elseif firstReset == 4 then
        newFace = "(}:o\\)"
        firstReset = 5
      elseif firstReset == 5 then
         newFace = "({:o|)"
        firstReset = 6
      elseif firstReset == 6 then
        newFace = "({:o))"
        firstReset = 7
      elseif firstReset == 7 then
        newFace = "({:op)"
        firstReset = 2
      end

      if firstReset ~= 1 and ifINFO ~= -1 and ifFORMAT ~= -1 and ifEnabled ~= -1 then
        TeleMenuInfo.Lines.Text = string.format([[%s - %s]],newFace, ifINFO)
      elseif ifINFO == -1 and ifEnabled ~= -1 then
        TeleMenuInfo.Lines.Text = string.format([[%s]],newFace)
