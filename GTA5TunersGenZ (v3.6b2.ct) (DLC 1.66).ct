    local function getAndSaveTele(sILENT)
      local fname = string.format('%s/GTA_GNullTeleports.ini',TrainerOrigin)
      local fs = io.open(fname, "r");

      if fs == nil then
        if sILENT ~= 1 then
          TELEMenuStatus.Lines.Text  = 'Error opening saved teleports file. No teleports saved? Permissions Error?'
        end
        return
      else
        local teleCount = 0
        local loadTemp = { }

        for line in io.lines(fname) do
          if line == nil or line == '' then goto continue end
          local tst1,tst2=string.find(line,':')
          if not tst1 or not tst2 then goto continue end
          local tLocation = string.sub(line,1,tst1-1)
          if not tLocation then goto continue end
          local tBuffer = string.sub(line,tst1+1,-1)
          local tst1,tst2=string.find(tBuffer,',')
          if not tst1 or not tst2 then goto continue end
          local tX = string.sub(tBuffer,1,tst1-1)
          if type(tonumber(tX)) == 'nil' then goto continue end
          local tBuffer = string.sub(tBuffer,tst1+1,-1)
          local tst1,tst2=string.find(tBuffer,',')
          if not tst1 or not tst2 then goto continue end
          local tY = string.sub(tBuffer,1,tst1-1)
          local tZ = string.sub(tBuffer,tst1+1,-1)
          if type(tonumber(tY)) == 'nil' or type(tonumber(tZ)) == 'nil' then goto continue end
          teleCount = teleCount + 1
          table.insert(loadTemp,{tLocation, tX, tY, tZ})
          ::continue::
        end

        fs:close()

        if teleCount == 0 then
          tbl_MyLocations = { }
          if sILENT ~= 1 then
            loadTELEPRESETS(string.format([[Error: No valid teleport entries found in GTA_GNullTeleports.ini]]))
          end
          return
        end

        if teleCount &gt; 0 then
          isTelePre = 2

          if sILENT ~= 1 then teleLOCSELDEL.Caption = "No Location Selected" end

          table.sort(loadTemp, function(a, b) return a[1] &lt; b[1] end)

          newTELELIST.items.clear()
          tbl_MyLocations = { }
          for k, v in pairs(loadTemp) do
            table.insert(tbl_MyLocations, {v[1], v[2], v[3], v[4]})
            if sILENT ~= 1 then newTELELIST.items.add(string.format('%s',v[1])) end
          end

          local doWriteTest = do_reWriteTelefile()

          if doWriteTest ~= 2 then
             if doWriteTest == 0 then
               check_MyCoordTimer(0)
               if sILENT ~= 1 then reset_TELEMenu(string.format([[Error: Unknown error while attempting to write to file: (%s)]], fname)) end
             elseif doWriteTest == 1 then
               check_MyCoordTimer(0)
               if sILENT ~= 1 then reset_TELEMenu(string.format([[Error: Unable to write teleport locations to file: (%s). Permissions error?]], fname)) end
             end
          elseif sILENT ~= 1 then
            TELEMenuStatus.Lines.Text  = string.format([[Loaded a total of (%i) personal teleport locations.]], teleCount)
            reset_TELEMenu(string.format([[Note: In order to change name or xyz, the teleport location must be highlighted in the list.]]))
            runTempTimer()
          end
        end
      end
    end

    local function do_aDDRemove(idNumber, aDDRemove)
      local tmp_s = { }
      local cntEntries = 0
      local fname = string.format('%sGTA_GNullTeleports.ini',TrainerOrigin)

      if aDDRemove == 1 then
        for k,v in pairs(tbl_MyLocations) do if idNumber ~= k then table.insert(tmp_s, {v[1], v[2], v[3], v[4]}) end end
        table.sort(tmp_s, function(a, b) return a[1] &lt; b[1] end)
        newTELELIST.items.clear()
        teleLOCSELDEL.Caption = "No Location Selected"
        tbl_MyLocations = { }
        for k,v in pairs(tmp_s) do
          table.insert(tbl_MyLocations, {v[1], v[2], v[3], v[4]})
          newTELELIST.items.add(string.format('%s',v[1]))
          cntEntries = cntEntries + 1
        end
      else
        if isTelePre == 2 then
          newTELELIST.items.clear()
          teleLOCSELDEL.Caption = "No Location Selected"
        end

        table.sort(tbl_MyLocations, function(a, b) return a[1] &lt; b[1] end)

        for k,v in pairs(tbl_MyLocations) do
          if isTelePre == 2 then newTELELIST.items.add(string.format('%s',v[1])) end
          cntEntries = cntEntries + 1
        end
      end

      if cntEntries == 0 then
        loadTELEPRESETS('Your teleport list is now empty. Auto Loading Presets and removing empty file!')
        tbl_MyLocations = { }
        os.remove(fname)
      else
        local doWriteTest = do_reWriteTelefile()

        if doWriteTest ~= 2 then
          if doWriteTest == 0 then
            check_MyCoordTimer(0)
            reset_TELEMenu(string.format([[Error: Unknown error while attempting to write to file: (%s)]], fname))
          elseif doWriteTest == 1 then
            check_MyCoordTimer(0)
            reset_TELEMenu(string.format([[Error: Unable to write teleport locations to file: (%s)]], fname))
          end
        end
      end
    end

